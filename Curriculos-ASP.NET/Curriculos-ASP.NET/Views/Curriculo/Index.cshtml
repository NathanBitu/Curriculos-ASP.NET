@model List<CurriculoViewModel>;

<h2>Listagem de Currículos</h2>
<br />

<a href="/curriculo/create" class="btn btn-success">Novo Registro</a>
<br />
<br />

<table class="table table-striped table-responsive">
    <tr>
        <th>Nome</th>
        <th>CPF</th>
        <th>Edição</th>
        <th>Deletar</th>
    </tr>

    @foreach (var curriculo in Model)
    {
        <tr>
            <td> @curriculo.nome</td>
            <td> @curriculo.cpf</td>
            <td><a href="/curriculo/Edit?cpf=@curriculo.cpf">Editar</a></td>
            <td><a href="/curriculo/Delete?cpf=@curriculo.cpf">Apagar</a></td>
        </tr>
    }

</table>